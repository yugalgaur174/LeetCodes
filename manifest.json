{
    "manifest_version": 3,
    "name": "LeetCode to GitHub Pusher",
    "version": "1.0",
    "description": "Automatically push your LeetCode solutions to GitHub.",
    "permissions": ["identity", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  